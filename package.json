{
  "name": "es6console",
  "version": "2.0.0",
  "description": "Try out ES6 JavaScript transpilers",
  "main": "src/main.js",
  "engines": {
    "node": ">=7.0.0",
    "npm": "^4.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "debug": "better-npm-run debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:staging": "better-npm-run deploy:staging",
    "deploy:prod": "better-npm-run deploy:prod",
    "lambda_deploy:staging": "better-npm-run lambda_deploy:staging",
    "lambda_deploy:prod": "better-npm-run lambda_deploy:prod",
    "sync": "better-npm-run sync",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "postinstall": "better-npm-run compile"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server -w server -w config -w bin",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "debug": {
      "command": "node debug bin/dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "sync": {
      "command": "node bin/deploy.js",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "lambda_deploy:staging": {
      "command": "cd lambda/storage; sls deploy -s dev",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "lambda_deploy:prod": {
      "command": "cd lambda/storage; sls deploy -s prod",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile && npm run sync",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:staging": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "staging",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "Matthisk Heimensen <m@tthisk.nl> (http://matthisk.nl)",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "~6.22.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.13",
    "body-parser": "~1.16.1",
    "classnames": "~2.2.5",
    "codemirror": "~5.27.0",
    "compression": "^1.6.2",
    "css-loader": "^0.26.0",
    "cssnano": "^3.7.4",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.0",
    "ip": "^1.1.2",
    "json-loader": "^0.5.4",
    "node-sass": "^9.0.0",
    "normalize.css": "^5.0.0",
    "pg": "~6.1.2",
    "postcss-loader": "^1.1.0",
    "prepack": "~0.2.0",
    "raven-js": "~3.15.0",
    "raw-loader": "~0.5.1",
    "react": "^15.0.0",
    "react-codemirror": "~0.3.0",
    "react-dom": "^15.0.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-select": "~1.0.0-rc.3",
    "redux": "^3.6.0",
    "redux-api-middleware": "~1.0.3",
    "redux-logger": "~2.8.1",
    "redux-thunk": "^2.0.0",
    "regenerator": "~0.8.26",
    "sass-loader": "^4.0.0",
